<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>HttpsRequest</servlet-name>
    <servlet-class>com.ntrcb.servlet.HttpsRequest</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Service</servlet-name>
    <servlet-class>com.ntrcb.servlet.Service</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Upvote</servlet-name>
    <servlet-class>com.ntrcb.servlet.Upvote</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>QueryJifen</servlet-name>
    <servlet-class>com.ntrcb.servlet.QueryJifen</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>HttpsRequest</servlet-name>
    <url-pattern>/servlet/HttpsRequest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Service</servlet-name>
    <url-pattern>/servlet/Service</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Upvote</servlet-name>
    <url-pattern>/servlet/Upvote</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QueryJifen</servlet-name>
    <url-pattern>/servlet/QueryJifen</url-pattern>
  </servlet-mapping>

</web-app>